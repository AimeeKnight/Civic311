extends ../shared/template

block content
  .row
    .small-12.columns
      form#forms(method='post', action='/admin/confirm')
        fieldset
          legend= title
          label Email
          input(type='email', name='email', required)
          label Password
          input(type='password', name='password', required)
          label Employee Access Token
          input(type='text', name='token', required)
          button.radius.small Complete Registration

